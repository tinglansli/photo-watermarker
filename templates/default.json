{
  "watermark_type": "text",
  "text": "@TingLans",
  "font_family": "Microsoft YaHei UI",
  "font_px": 128,
  "bold": false,
  "italic": false,
  "text_color": "#FFFFFF",
  "opacity": 70,
  "outline": true,
  "outline_px": 2,
  "outline_color": "#000000",
  "shadow": true,
  "shadow_dx": 2,
  "shadow_dy": 2,
  "shadow_color": "rgba(0,0,0,128)",
  "image_path": "",
  "image_scale_percent": 40,
  "image_opacity": 70,
  "rotation_deg": 0,
  "anchor": "br",
  "pos_ratio_x": 0.75,
  "pos_ratio_y": 0.75,
  "export": {
    "output_dir": "",
    "prevent_export_to_source": true,
    "out_format": "PNG",
    "jpeg_quality": 90,
    "resize_mode": "none",
    "resize_value": 100,
    "name_rule": "suffix",
    "name_value": "_watermarked"
  }
}